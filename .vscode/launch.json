    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run model test SLN",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/demo_SLN/test_model.py",
            "console": "integratedTerminal",
        },
        {
            "name": "Server Box: Run flask app in dev post",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "server/app:create_app()",
                "FLASK_ENV": "production",
            },
            "args": [
                "run",
                "--host",
                "0.0.0.0",
                "--no-reload",
            ]
        }
    ]
}
